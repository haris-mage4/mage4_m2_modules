<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
   <body>
      <referenceBlock name="customer_account_navigation">
         <block class="Magento\Framework\View\Element\Html\Link\Current" name="benefitsme_documents">
            <arguments>
               <argument name="path" xsi:type="string">benefitsme/customer/documents</argument>
               <argument name="label" xsi:type="string">Documents</argument>
            </arguments>
         </block>
      </referenceBlock>
      <referenceBlock name="customer_account_navigation">
         <block class="Magento\Framework\View\Element\Html\Link\Current" name="benefitsme_credit">
            <arguments>
               <argument name="path" xsi:type="string">benefitsme/customer/credit</argument>
               <argument name="label" xsi:type="string">Credit Information</argument>
            </arguments>
         </block>
      </referenceBlock>
      <referenceBlock name="customer_account_navigation"> 
          <block class="Magento\Framework\View\Element\Html\Link\Current" name="customer-account-navagation-signout-link">
          <arguments>
               <argument name="path" xsi:type="string">customer/account/logout</argument>
	       <argument name="label" xsi:type="string">Logout</argument>
	       <argument name="sortOrder" xsi:type="number">99999</argument>
          </arguments>
          </block>
       </referenceBlock>
   </body>
</page>
